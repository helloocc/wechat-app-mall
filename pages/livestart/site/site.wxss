#page {
  margin: 0 24rpx;
}

.header-box {
  width: 100%;
  /* padding-top: 20rpx; */
  margin-bottom: 12rpx;
}

.detail-bg {
  width: 275rpx;
  display: block;
  top: 48rpx;
  left: 40rpx;
  position: absolute;
}

.detail-bg image {
  border-radius: 8px;
  width: 200rpx;
  height: 200rpx;
  background: #ffffff;
}

.detail-box {
  background: #ffffff;
  /* border-radius: 4px 40px 0 0; */
  padding: 42rpx 20rpx 32rpx 260rpx;
  min-height: 220rpx;
}

.detail-title {
  font-size: 36rpx;
  color: #333;
  font-weight: 600;
  margin-bottom: 16rpx;
}

.detail-desc {
  font-size: 26rpx;
  color: #999999;
  margin-left: 16rpx;
  padding: 20rpx;
  border-radius: 8px;
  background-image: linear-gradient(129deg,
      #d3f6e7 0%,
      #f6faed 49%,
      #ffffff 82%);
}

.detail-day,
.detail-site {
  font-size: 26rpx;
  line-height: 36rpx;
  color: #666666;
  margin-bottom: 20rpx;
}

.detail-day image,
.detail-site image {
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
  margin-right: 14rpx;
}

.detail-line {
  margin: 0 24rpx 30rpx;
  border-bottom: 1rpx solid #e5e5e5;
  background-color: #fff;
}

.join-room {
  background-color: rgb(98,178,143);
  /* border:rgb(82, 166, 124) ; */
  width: 156rpx;
  height: 60rpx;
  text-align: center;
  color: #fff;
  line-height: 60rpx;
  border-radius: 10rpx !important;
  font-size: 20rpx;
  font-weight: 600;
  /* box-shadow: rgba(98, 178, 143, 0.4) 0px 4px 8px; */
  margin-left: 0rpx;
}

/* month-header START */
/* 月份电梯样式 */
.month-header {
  background-color: #fff;
  padding: 8rpx 0;
  height: 32px;
  width: 100%;
  position: sticky;
  top: -1px;
  z-index: 999;
}

.month-header .month-header-list {
  display: flex;
  justify-content: start;
  flex-direction: row;
  flex-wrap: nowrap;
}

.month-header-item {
  padding: 12px 16px 12px 0 !important;
  margin: 0 8px;
  font-size: 32rpx !important;
  white-space: nowrap;
  color: #bbb !important;

  display: flex;
  align-items: center;
  box-sizing: border-box;
}

.ant-tabs-bar-basic.ant-tabs-bar-active:after {
  background: none !important;
}

.ant-tabs-bar-scroll-view {
  height: 46px !important;
  justify-content: flex-start !important;
}

.goLiveshow {
  padding: 60rpx 0;
  text-align: center;
  color: rgb(75, 74, 74);
  font-size: 26rpx;
  font-weight: 600;
}

.month-header .active {
  background-image: url("data:image/jpeg;base64,");
  background-size: 90%;
  background-position: 0px 12px;
  background-repeat: no-repeat;
  color: #333 !important;
}

/* month-header END */